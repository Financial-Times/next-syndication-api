<?xml version="1.0"?>
<document>
	<nitf>
		<head>
			<title>{{title}}</title>
		</head>
		<body>
			<body.head>
				<hedline>
					<hl1>{{title}}</hl1>
				</hedline>
				<byline>
					<bytag>{{byline}}</bytag>
				</byline>
			</body.head>
			<body.content>
				<block>
					{{bodyXML}}
					<p>Copyright The Financial Times Limited {{year}}</p>
					<p>&copy; {{year}} The Financial Times Ltd. All rights reserved. Please do not copy and paste FT articles and redistribute by email or post to the web.</p>
				</block>
			</body.content>
		</body>
	</nitf>
	<xn:Resource xmlns:xn="http://www.xmlnews.org/namespaces/meta#">
		<xn:resourceID>{{id}}</xn:resourceID>
		<xn:publicationTime>{{publishedDate}}</xn:publicationTime>
		<xn:title>{{title}}</xn:title>
		<xn:vendorData>FT______:Url={{webUrl}}</xn:vendorData>
	</xn:Resource>
</document>
